    "babel-plugin-import": "^1.13.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.26.0",
    "eslint-config-next": "13.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "prettier": "^2.7.1",
    "rimraf": "^6.0.1",
    "typescript": "^4.8.4"
  }
