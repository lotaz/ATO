{
  "name": "cong-binh-web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "node server.config.js",
    "build": "next build",
    "start": "NODE_ENV=production node server.config.js",
    "build:dev": "cross-env NEXT_ENV=development next build",
    "build:prod": "cross-env NEXT_ENV=production next build",
    "start:dev": "cross-env NEXT_ENV=development next start",
    "start:prod": "cross-env NEXT_ENV=production next start",
    "lint": "next lint",
    "clean": "rimraf .next out",
    "build-and-start:dev": "npm run clean && npm run build:dev && npm run start:dev",
    "build-and-start:prod": "npm run clean && npm run build:prod && npm run start:prod"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "prettier --write --ignore-unknown"
    ],
    "*.+(json|css|md)": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@emotion/cache": "^11.10.5",
    "@emotion/react": "^11.10.5",
